{
    "presets": [
        "es2015",
        "stage-2"
    ],
    "plugins": [
        [
            "transform-decorators-legacy"
        ],
        [
            "transform-react-jsx"
        ],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}
